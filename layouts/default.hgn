<html>
    <head>
        <title>{{metadata.title}}</title>
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Lato:400,700,400italic" />
        <link rel="stylesheet" media="all" href="{{metadata.cssPath}}" />
    </head>
    <body>

        <section id="container">

        <h1>{{metadata.title}}</h1>

            {{#contents}}

            <table class="section">
                <thead>
                    <tr class="section_header">
                        <th class="empty"></th>
                        <th>{{{section}}}</th>
                    </tr>   
                </thead>
                <tbody>
                    {{#items}}
                    <tr class="item">
                        <td class="label">{{{label}}}</td>
                        <td>{{{content}}}</td>
                    </tr>
                    {{/items}}
                </tbody>
            </table>
            {{/contents}}

        </section>

    </body>
</html>